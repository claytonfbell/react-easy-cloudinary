{
  "version": "1.0.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claytonfbell/react-easy-cloudinary.git"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "standard-version"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "name": "react-easy-cloudinary",
  "author": "Clay Bell",
  "module": "dist/react-easy-cloudinary.esm.js",
  "size-limit": [
    {
      "path": "dist/react-easy-cloudinary.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-easy-cloudinary.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.3",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "husky": "^7.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "size-limit": "^6.0.3",
    "standard-version": "^9.3.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "cloudinary-react": "^1.7.0",
    "prop-types": "^15.7.2"
  }
